{
  "manifest_version": 2,

  "name": "InCHROMEntor",
  "description": "This extension allows you to easily increment/decrement URLs for fast browsing of incrementally-named pages.",
  "version": "0.4.1",
  "background":{
		"scripts": ["background.js"]
	},
  "content_scripts":[{
  "matches":["<all_urls>"],
  "js": ["jquery-1.9.1.min.js", "allpages.js"]
  
  }], 
  
  "options_page": "options.html",

  "permissions": [
    "tabs"
  ],
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
